<?xml version='1.0' encoding='UTF-8' ?>
<mots xml:lang='ar'>
<mot name='﻿جَغِبٌ'>
<def> إتْباعٌ لِشَغِبٍ.</def>
</mot>
<mot name=' عَزَبٌ لَزَبٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='البَليثُ'>
<def> كَلأَ عامَيْن أسْوَدُ كالدَّرِينِ، وإتْباعُ"دَمِيثٍ".</def>
</mot>
<mot name='خَبيثٌ لَبيثٌ نَبيثٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='كَلامٌ غَثٌّ نَثٌّ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='الدَّاجَةُ'>
<def> تُبَّاعُ العَسْكَرِ، وما صَغُرَ من الحوائِجِ، أو إتْباعٌ للحاجَةِ.</def>
</mot>
<mot name='سَمْجٌ لَمْجٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='شَحيحٌ بَحيحٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='سُلاطِحٌ بلاطِحٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='قُبْحاً له وشُقْحاً'>
<def> إِتْباعٌ، أو بِمَعْنًى، ويُفْتَحانِ.</def>
</mot>
<mot name='صُلاطِحٌ بُلاطِحٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='الضِّيحُ'>
<def> الضِّحُّ، وإتْباعٌ لِلرِّيحِ.</def>
</mot>
<mot name='مَلِيحٌ قَزِيحٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='شقيحٌ لَقِيحٌ'>
<def> إتْبَاعٌ.</def>
</mot>
<mot name='شَحيحٌ نَحيحٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='واحِدٌ قاحِدٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='رُطَبٌ ثَعْدٌ مَعْدٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='كثيرٌ أثيرٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='كثيرٌ بثيرٌ'>
<def> إِتْباعٌ، ويُفْرَدُ.</def>
</mot>
<mot name='كَثيرٌ بجيرٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='كثيرٌ بَذيرٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' حارٌّ جارٌّ'>
<def> إتباعٌ.</def>
</mot>
<mot name=' كثيرٌ بَجِيرٌ عَمِيرٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='الحِجارَةُ والمِدارَةُ'>
<def> إتباعٌ.</def>
</mot>
<mot name='وعِفْرٌ نِفْرٌ'>
<def> وعِفِرٌّ، نِفِرٌّ، وعِفْرِيتَةٌ نِفْريتَةٌ, إتْباعٌ.</def>
</mot>
<mot name='حَقيرٌ نَقيرٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='شَعَرٌ مَعِرٌ وعِرٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='رجلٌ وعْرُ المَعْروفِ'>
<def> قليلُهُ. ويُقالُ قليلٌ وعْرٌ، إِتْباعٌ.</def>
</mot>
<mot name='فَقِيرٌ وقِيرٌ'>
<def> تَشْبيهٌ بِصِغار الشاءِ، أو إِتْباعٌ.</def>
</mot>
<mot name='حَرَّانُ يَرَّانُ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='اليَرَّةُ'>
<def> النارُ. ويقالُ هذا الشَّرُّ واليَرُّ، كأنه إتْباعٌ.</def>
</mot>
<mot name='عَجوزٌ لَزوزٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='إنه لَعَوِزٌ لَوِزٌ'>
<def> مُحْتَاجٌ</def>
</mot>
<mot name=' عَزيزٌ مَزِيزٌ'>
<def> إتباعٌ.</def>
</mot>
<mot name='وجُوعاً له وجُوساً'>
<def> إتْباعٌ.</def>
</mot>
<mot name='رَجُلٌ أخْرَسُ أضْرَسُ'>
<def> إتْباع.</def>
</mot>
<mot name='دونَ هذا الأمْرِ عِكاسٌ ومِكَاسٌ، بكسرهِمَا'>
<def> وهو أن تأخُذَ بناصِيَتِهِ ويأخُذَ بِنَاصِيَتِكَ،أو هو إتْباعٌ.</def>
</mot>
<mot name='عابِسٌ كابِسٌ'>
<def> إتْبَاعٌ.</def>
</mot>
<mot name='لِيَفْسٌ، بكسر اللامِ وفتح الياءِ'>
<def> إتباعٌ لِحِيَفْسٍ، أي شُجاعٌ.</def>
</mot>
<mot name='عَطْشَانٌ نَطْشَانٌ'>
<def> إتْبَاعٌ.</def>
</mot>
<mot name='عريضٌ أريضٌ'>
<def> إتباع.</def>
</mot>
<mot name=' حُطائطٌ بُطائطٌ'>
<def> إتباعٌ.</def>
</mot>
<mot name=' شَيطانٌ لَيْطانٌ'>
<def> أو هو إِتْباعٌ.</def>
</mot>
<mot name='النَّافِطَةُ'>
<def> الماعِزَةُ، أو إتْبَاعٌ للعافِطَةِ.</def>
</mot>
<mot name=' فَظٌّ بَظٌّ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' جاؤُوا كلُّهُمْ أجْمَعُونَ أكْتَعُونَ أبْصَعُونَ أبْتَعُونَ'>
<def> إتْبَاعاتٌ لأَجْمَعِينَ لا يَجِئْنَ إلاَّ على إثْرِها، أو تَبْدَأُ بأيَّتِهِنَّ شِئْتَ بَعْدَهَا.</def>
</mot>
<mot name=' حَسَنْ بَسَنْ'>
<def>إتْباعٌ.</def>
</mot>
<mot name='السَّلْقَعُ'>
<def> المكانُ الحَزْنُ، أو إتْباعٌ لِبَلْقَعٍ.</def>
</mot>
<mot name=' رأيْتُهم أجمعينَ أكْتَعِينَ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' شُجاعٌ نُجاعٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' جائعٌ نائعٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='الأفُّ ، والتُّفُّ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='التُّفُّ'>
<def> وسَخُ الظُّفُرِ، أو إتْباعٌ لأُفٍّ.</def>
</mot>
<mot name=' خُفافٌ ذُفافٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name=' ضعيفٌ نَعيفٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' خاذِقٌ باذِقٌ'>
<def> إتْبَاعٌ.</def>
</mot>
<mot name='ضَيِّقٌ لَيِّقٌ عَيِّقٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='لا بارَكَ اللّهُ فيه، ولا تارَكَ، ولا دارَكَ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' لا بارَكَ اللّهُ تعالى فيه ولا دارَكَ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' والضلالُ ابنُ الأَلالِ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='حِلٌّ وبِلٌّ'>
<def> أو هو إتْباعٌ.</def>
</mot>
<mot name='وَليلٌ بَلِيلٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='ضالٌّ تالٌّ'>
<def> والضَّلالَةُ والتَّلالَةُ، والضَّلالُ ابنُ التَّلالِ إتْباعٌ.</def>
</mot>
<mot name='رَغْماً دَغْماً شِنَّغْماً'>
<def> إتْباعاتٌ.</def>
</mot>
<mot name='راغِمٌ داغِمٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='رَغْماً له سِنَّغْماً'>
<def> إِتْباعٌ، أو هو بالشينِ.</def>
</mot>
<mot name='رَغْماً له شِنَّغْماً'>
<def> إتْبَاعٌ.</def>
</mot>
<mot name='فَدْمٌ ثَدْمٌ لَدْمٌ'>
<def> إِتْباعٌ.</def>
</mot>
<mot name='بَسَنٌ،'>
<def> إتْباعٌ لحَسَنِ.</def>
</mot>
<mot name='مجنونٌ مَشْعونٌ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='لا دَرَيْتَ ولا ائْتَلَيْتَ'>
<def> إتْباعٌ.</def>
</mot>
<mot name='حَظِيتِ المرأَةُ وبَظِيَتْ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' بَيَّاكَ اللُّه'>
<def> أضْحَكَكَ اللُّه، أو قَرَّبَكَ، أو جاءَ بكَ، أو بَوَّأَكَ، أو إتْباعٌ لحَيَّاكَ.</def>
</mot>
<mot name=' عَيِيٌّ شَيِيٌّ وشَوِيٌّ'>
<def> إتْباعٌ. وما أعْياهُ وأشْياهُ وأشْواهُ.</def>
</mot>
<mot name=' خَظَا بَظَا كَظَا إتْباعٌ'>
<def> للصُلْبِ المُكْتَنِزِ.</def>
</mot>
<mot name=' خَصِيٌّ بَصِيٌّ لَصِيٌّ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' شَقِيٌّ لَقِيٌّ'>
<def> إتْباعٌ.</def>
</mot>
<mot name=' ثِقَةٌ نِقَةٌ'>
<def> إتْباعٌ.</def>
</mot>
</mots>
